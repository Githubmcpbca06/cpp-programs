#include<iostream>
using namespace std;
int main()
{
	int **p,r,c,nr,nc;
  cout<<"Enter the  number  of  row: "; cin>>nr;
  cout<<"\nEnter the number of cols: "; cin>>nc;
  p=new int*[nr];
  for(r=0;r<nr;r++)
    p[r]=new int*[nr];
  for(r=0;r<nr;r++)
    {
      for(c=0;c<nc;c++)
        {
          cin>>p[r][c];
        }
    }
  for(r=0;r<nr;r++)
    {
      for(c=0;c<nc;c++)
        {
          cout<<p[r][c]<<ends;
        }
        cout<<"endl";
    }
    
}
