#include<iostream>
using namespace std;
class parent
{
public:
    void show()
          {
            cout<<"In Parent!";
          }
};
class child:public parent
{
public:
    void show()
          {
            cout<<"In Child!";
          }
};
int main()
{
  parent *p,p1;
  child c1;
  p=&c1;
  p->show();
}
